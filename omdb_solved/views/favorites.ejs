<% favorites.forEach(function(favorite) { %>
  <a href="/movies/<%= favorite.imdbId %>"><h1><%= favorite.title %> - <small><%= favorite.year %></small></h1></a>
  <a class="btn btn-primary" href="/favorites/<%= favorite.imdbId %>/comments">See Comments</a>
  <a class="btn btn-primary" href="/favorites/<%= favorite.imdbId %>/tag">add a tag</a>
<% }); %>
